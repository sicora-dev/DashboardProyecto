<UserControl x:Class="DashboardTienda.UserControls.Legend"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <StackPanel Orientation="Horizontal">
        <Border Width="12" Height="12" Background="#0074ff" CornerRadius="3"/>
        <TextBlock Text="2023" VerticalAlignment="Center" Margin="7 0 20 0" Foreground="{DynamicResource TextBrush}"/>

        <Border Width="12" Height="12" Background="#ff6d48" CornerRadius="3"/>
        <TextBlock Text="2024" VerticalAlignment="Center" Margin="7 0 20 0" Foreground="{DynamicResource TextBrush}"/>
    </StackPanel>
</UserControl>
