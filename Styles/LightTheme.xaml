<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="BackgroundColor"
                     Color="#F4F4F6" />
    <SolidColorBrush x:Key="TextColor"
                     Color="#353535" />
    <SolidColorBrush x:Key="SecondaryColor"
                     Color="#E6E6E9" />
    <SolidColorBrush x:Key="TertiaryColor"
                     Color="#9999A1" />
    <SolidColorBrush x:Key="HighlightColor"
                     Color="#F46036" />
    
    <Style TargetType="TextBlock" >
        <Setter Property="Foreground"
                Value="{DynamicResource TextColor}" />
    </Style>
</ResourceDictionary>